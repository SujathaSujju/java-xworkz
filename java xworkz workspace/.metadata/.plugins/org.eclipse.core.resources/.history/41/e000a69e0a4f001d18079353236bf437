package com_xworkz_javaa.polymorphism.methodOveriding.populationmethod;

public class Population {

	public static void main(String[] args) {
		 India india = new India();
		 Karnataka karnataka = new Karnataka();
		 AndhraPradesh andhrapradesh = new AndhraPradesh();
		  System.out.println(india.getpopulations());
		  System.out.println(karnataka.getpopulations());
		  System.out.println(andhrapradesh.getpopulations());
		  
	}
}
