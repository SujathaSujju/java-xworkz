package com_xworkz_javaa.inheritance.Human_heart;

public class HumanHeartDemo {
	public static void main(String[] args) {
		
		Human obj1 = new Human();
		
		obj1.Bodyparts = "Heart";
		obj1.Temperature = 35;
	
		System.out.println("human heartbeat is"+obj1.Bodyparts);
			
		System.out.println("Normal Human Body Temperature is"+obj1.Temperature);
		
	}
	
	

}
